{
  "name": "links",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch": "speclate --watch",
    "offline": "npm run js && npm run html",
    "build": "mkdir -p docs && npm run update && npm run js && npm run html",
    "html": "speclate --build ",
    "style": "speclate --scss",
    "start": "speclate --dev 5009",
    "update": "node scripts/update.js",
    "js-dev": "npm run js && mv ./client/index-compiled.js ./docs/client/index-compiled.js",
    "js": "npm run prep-js && npm run js-app",
    "prep-js": "mkdir -p ./docs/client",
    "js-app-uncompiled": "browserify ./client/index.js  > ./docs/client/index-compiled.js",
    "js-app-all": "browserify ./client/index.js  | uglifyjs > ./client/index-compiled.js",
    "js-app": "browserify ./client/index.js  > ./client/index-compiled.js",
    "js-service-worker": "browserify ./client/service-worker.js > ./docs/service-worker.js",
    "js-watch": "watchify ./client/index.js -o ./client/index-compiled.js",
    "sass-watch": "node-sass ./pages/layout.scss  --watch",
    "import-blog": "node ./scripts/import-blog.js",
    "clean": " rm -Rf posts &&  rm -Rf docs"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "appcache-nanny": "^1.1.0",
    "async": "^2.6.2",
    "browserify": "14.x.x",
    "fs-readdir-recursive": "^1.1.0",
    "ga-browser": "^1.0.0",
    "gray-matter": "^4.0.2",
    "lodash": "^4.17.15",
    "moment": "^2.18.1",
    "node-html-encoder": "0.0.2",
    "raven-js": "^3.11.0",
    "showdown": "^1.9.0",
    "slugify": "^1.3.4",
    "speclate": "^19.3.0",
    "speclate-service-worker": "3.x.x",
    "superagent": "^3.5.2",
    "tailwindcss": "^1.2.0",
    "turndown": "^5.0.3",
    "uglify-js": "^2.4.10",
    "watchify": "^3.11.1"
  }
}
